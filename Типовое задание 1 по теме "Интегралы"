    case 80:
        integral=string.Format("integrate(x*exp(sqrt(x)),x)");
        answer=string.Format("2*(sqrt(x)*(x+6)-3*x-6)*exp(sqrt(x))");
        break;
}
string question="\\displaystyle " + Helper.Latex(integral,false);
