    case 67:
        integral=string.Format("integrate((log(x)-1)/(log(x)**2),x)");
        answer=string.Format("x/log(x)");
        break;
        
    case 68:
        integral=string.Format("integrate((log(x))/(log(x)+1)**2,x)");
        answer=string.Format("x/(log(x)+1)");
