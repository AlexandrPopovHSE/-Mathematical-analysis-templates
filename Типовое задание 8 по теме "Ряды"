func = "(-1)^n \\frac {n^2+1}{\\sqrt[3]{n^7}}";
ans = 1;
break;
case 7:
a = 1;
func = "(-1)^n \\frac {n^3}{(\\ln 3)^n}";
ans = 2;
break;
case 8:
a = 1;
func = "(-1)^n \\frac{n}{n^2+\\ln n}";
ans = 1;
