case 9:
a = 1;
func = "(-1)^n \\frac n {\\sqrt[3]{n^4}+\\sqrt n}";
ans = 1;
break;
case 10:
a = 1;
func = "(-1)^n \\sqrt[5] n \\sin \\frac 1 n";
ans = 1;
