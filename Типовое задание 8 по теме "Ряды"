case 20:
a = 1;
func = "(-1)^n \\sqrt[3]{n}\\sin \\frac 1 {\\sqrt n}";
ans = 1;
break;
case 21:
a = 1;
func = "(-1)^n \\sqrt n \\tan \\frac 1 {\\sqrt n}";
ans = 0;
break;
case 22:
