string question,correct_answer,func;

Random rnd = new Random();
int k=rnd.Next(0,27);
int ans,a;

switch (k){
case 0:
a = 1;
func = "(-1)^n (exp(-\\frac 1 {\\sqrt n}) -1)";
ans = 1;
break;
case 1:
a = 1;
func = "(-1)^n (\\sqrt{n+1} - \\sqrt n)";
ans = 1;
break;
case 2:
a = 1;
func = "(-1)^n \\left(\\sqrt{1+ \\frac 1 {\\sqrt n}} -1\\right)";
